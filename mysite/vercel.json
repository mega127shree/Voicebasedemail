{
    "builds":[{
        "src":"mysite/wsgi.py",
        "use":"@vercel/python",
        "config":{"maxLambdaSize":"30mb","runtime":"python3.9"}
    },{
        "src":"build_files.sh",
        "use":"@vercel/static-build",
        "config":{
            "distDir":"staticfiles_build"
        }
    }],
    "routes":[{
        "src":"/assets/(.*)",
        "dest":"mysite/wsgi.py"
    },
        {
            "src":"/(.*)",
            "dest":"mysite/wsgi.py"
        }
    ]
}
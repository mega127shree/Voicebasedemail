{
    "builds":[{
        "src":"mysite/wsgi.py",
        "use":"@vercel/python",
        "config":{"maxLambdaSize":"30mb","runtime":"python3.9"}
    }],
    "routes":[{
        "src":"/assets/(.*)",
        "dest":"mysite/wsgi.py"
    },
        {
            "src":"/(.*)",
            "dest":"mysite/wsgi.py"
        }
    ]
}
